.darkmode--activated {

  .tipue_search_content_text {
    color: #ffffff;
  }

  .tipue_search_content_title a {
      color: #ffffff;
  }

  .v[data-class='v'] p {
    color: #ccc;
  }

  .vpreview h4, .vpreview p {
    color: #444 !important;
  }

  .v[data-class='v'] code, .v[data-class='v'] pre {
    color: #ccc;
    background-color: #282c34;
  }

}
