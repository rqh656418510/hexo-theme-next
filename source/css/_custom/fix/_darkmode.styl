.darkmode--activated {

  .tipue_search_content_text {
      color: #fff;
  }

  .tipue_search_content_title a {
      color: #fff;
  }

  .v[data-class='v'] code {
      color: #ccc;
      background-color: #282c34;
  }
  
  .admonition {
      text-color: #ccc;
      background-color: #333;
  }

  blockquote {
      text-color: #ccc;
      background-color: #333;
  }
  
  .moon-menu {
      --moon-cricle: #fff !important;
      --moon-item-bg-color: #fff !important;
  }

  .moon-menu-icon, .moon-menu-item, .moon-menu-text {
      color: #000;
  }

}
